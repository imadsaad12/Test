CREATE TABEL models (
    model_id SERIAL PRIMARY KEY,
    slug VARCHAR(255),
    image VARCHAR(255),
    name VARCHAR(255),
    type VARCHAR(255)
) ;

CREATE TABEL comments(
    comment_id SERIAL PRIMARY KEY,
    model_id  INT,
    name VARCHAR(255),
    date VARCHAR(255),
    comment VARCHAR(255)
);

CREATE TABEL rating(
    id SERIAL PRIMARY KEY,
    model_id  INT,
    name VARCHAR(255),
    stars INT 
);


